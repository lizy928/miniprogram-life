.container {
    display: flex;
    flex-direction: column;
}

/*打卡圈子名称输入框*/
.input-project-name {
    width: auto;
    height: 30%;
    margin: 100px 10% 0 10%;
    text-align: center;
    font-size: 20px;
    color: black;
}

/*输入框下边的横线*/
.input-project-name > view {
    width: 100%;
    height: 2px;
    background-color: lightseagreen;
}

/*打卡圈子隐私类型选择*/
.chose-project-privacy-type {
    width: auto;
    height: 30%;
    padding: 240px 15% 0 15%;

    font-size: 14px;
    color: black;

}
.radio-group radio {
    margin-bottom: 6px;
}

/*下一步按钮*/
.submitBtn {
    width: auto;
    height: 40%;
    margin: 0 5% 0 5%;
    padding-top: 20px;
}

